th: 40px;\n        height: 40px;\n        border-radius: 50%;\n        filter: grayscale(100%);\n        display: block;\n        overflow: hidden;\n        margin: 16px 10px;\n     }\n`\n\nconst Avatar = styled(motion.div)`\n`\nconst AuthorName = styled(motion.div)`\n`\nconst AutorNamePrefix = styled(motion.div)`\n    font-style: normal;\n    font-weight: 700;\n    color: #7a7a8c;\n`\n\nconst HalfCircle = styled.svg`\nposition: absolute;\nbottom: 0;\nleft: 0;\nwidth: 60px;\nheight: 48px;\nfill: none;\nstroke: #00d2ff;\nstroke-width: 8;\nstroke-linecap: round;\npointer-events: none;\n`\n\nconst container = {\n    hidden:{\n        opacity:0\n    },\n    show:{\n        opacity:1,\n        transition:{\n            type:\"spring\",\n            staggerChildren:0.5,\n            delayChildren: 0.5        \n        },\n    },\n};\n\nconst item = {\n    hidden:{\n        opacity:0\n    },\n    show:{\n        opacity:1,\n    }\n}\n\n\nfunction ExperienceComponent() {\n    return (\n        <>\n        \n        <CardList variants={container}\n            initial=\"hidden\"\n            animate=\"show\"\n\n        >\n        {Experiences.map(experience =>{\n            return <Card variants={item} key={experience.id} \n             \n            >\n            <CardHeader>\n                <p>{experience.date}</p>\n                <h2>{experience.JobTitle}</h2>\n               <h4>Tasks: </h4>\n               <ul>\n                   {experience.tasks.map((task,index)=>(\n                        <li key={index+20}>\n                            <p> {task}</p>\n                        </li>\n                   ))}\n               </ul>\n            </CardHeader>\n            \n            <CardInfo >\n                <Avatar>\n                    <img src={Me} />\n                </Avatar>\n                <HalfCircle  viewBox=\"0 0 106 57\">\n                    <path d=\"M102 4c0 27.1-21.9 49-49 49S4 31.1 4 4\"></path>\n                </HalfCircle>\n\n                <AuthorName>\n                    <AutorNamePrefix>\n                        <p>Company :</p>\n                    </AutorNamePrefix>{experience.companyName}\n                </AuthorName>\n            </CardInfo>\n\n        </Card>\n        })\n    }\n       \n        </CardList>\n        </>\n    )\n}\n\nexport default ExperienceComponent\n","/Users/macbookpro/Downloads/portfolio 2/src/js/src/data/ExperienceData.js",[],"/Users/macbookpro/Downloads/portfolio 2/src/js/src/components/MyProjectsPage.js",[],"/Users/macbookpro/Downloads/portfolio 2/src/js/src/App.js",["93","94"],"\nimport React, {useState} from 'react'\nimport { ThemeProvider } from 'styled-components';\nimport GlobalStyle from './global';\nimport {lightTheme, darkTheme} from './theme'\nimport {Route, Switch} from 'react-router-dom'\n\nimport AboutPage from './components/AboutPage';\nimport MyEducationPage from './components/MyEducationPage';\nimport MyProjectsPage from './components/MyProjectsPage'\nimport MainPage from './components/MainPage';\nimport MySkillsPage from './components/MySkillsPage'\nimport MyExperiencePage from './components/MyExperiencePage';\n\nfunction App() {\n\n  return (\n   <>\n    <ThemeProvider theme={lightTheme}>\n      <GlobalStyle />\n\n      <Switch>\n          <Route exact path=\"/\" component={MainPage}/>\n          <Route exact path=\"/about\" component={AboutPage}/>\n          <Route exact path=\"/skills\" component={MySkillsPage} />\n          <Route exact path=\"/education\" component={MyEducationPage}/>\n          <Route exact path=\"/project\" component={MyProjectsPage}/>\n          <Route exact path=\"/experiences\" component={MyExperiencePage}/>\n      </Switch>\n    </ThemeProvider>\n   </>\n  );\n}\n\nexport default App;\n","/Users/macbookpro/Downloads/portfolio 2/src/js/src/index.js",[],"/Users/macbookpro/Downloads/portfolio 2/src/js/src/reportWebVitals.js",[],"/Users/macbookpro/Downloads/portfolio 2/src/js/src/theme.js",[],"/Users/macbookpro/Downloads/portfolio 